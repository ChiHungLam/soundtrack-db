<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
	xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<ui:style>
		
	</ui:style>
	
	<g:HTMLPanel>
		<g:Label text="Serien-Details"/>
		<g:Grid>
			<g:row>
				<g:customCell>
					<g:Label text="Id"/>
				</g:customCell>
				<g:customCell>
					<g:LongBox ui:field="idField" readOnly="true"/>
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:Label text="Titel"/>
				</g:customCell>
				<g:customCell>
					<g:TextBox ui:field="titleField" readOnly="true"/>
				</g:customCell>
			</g:row>
		</g:Grid>
	</g:HTMLPanel>
</ui:UiBinder> 