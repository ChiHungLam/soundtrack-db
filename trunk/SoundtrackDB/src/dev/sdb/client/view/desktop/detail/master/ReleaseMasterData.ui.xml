<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
	xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<ui:style>
		.content {
			position: absolute;
			top: 10px;
			bottom: 10px;
			left: 10px;
			right: 10px;
		}
	</ui:style>
	
	<g:HTMLPanel>
		<g:HTML wordWrap="false" ui:field="content" addStyleNames="{style.content}"></g:HTML>
	</g:HTMLPanel>
</ui:UiBinder> 