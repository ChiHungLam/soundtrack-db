<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder
	xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<ui:style>
		.productionInfo {
			margin-top: 20px;
			text-shadow: 0 0 14px white;
			font-size: smaller;
			font-weight: bold;
		}
	</ui:style>
	
	<g:HTMLPanel>
		<g:FlowPanel>
			<g:Label text="(P) 2013 JooceWare" styleName="{style.productionInfo}"/>
		</g:FlowPanel>
	</g:HTMLPanel>
</ui:UiBinder> 